{
  "name": "rayenbou/ticket-bundle",
  "description": "A Symfony bundle to manage tickets",
  "license": "MIT",
  "type": "symfony-bundle",
  "authors": [
    {
      "name": "Rayen Boumaza",
      "email": "boumaza.rayen@outlook.fr",
      "role": "Author"
    }
  ],
  "autoload": {
    "psr-4": {
      "Rayenbou\\TicketBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Rayenbou\\TicketBundle\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.1",
    "symfony/framework-bundle": ">=6.4",
    "symfony/http-client": ">=6.4"
  },
  "config": {
    "optimize-autoloader": true
  },
  "extra": {
    "branch-alias": {
      "dev-main": "1.x-dev"
    }
  },
  "scripts": {
    "test": "phpunit"
  }
}
